<div class="container" style="margin-top: 20px; outline: 1px solid orange;">
	<div class="title_icon"><img src="/person.png"></div><h1>Add A New Case</h1>
    <div id='navigation' class="clearfix"></div>
	<div>
		<form class="form-horizontal" role="form" action="/people" method="post">
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="first_name">First Name:</label>
		    <div class="col-sm-10">
		      <input type="text" class="form-control" id="first_name" name="person[first_name]" placeholder="Enter First Name">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="last_name">Family/Last Name:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="last_name" name="person[last_name]" placeholder="Enter Family Name">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="middle_name">Middle Name:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="middle_name" name="person[middle_name]" placeholder="Enter Middle Name">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="middle_name">Nickname:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="nickname" name="person[nickname]" placeholder="Enter Nickname">
		    </div>
		  </div>
		  <div class="form-group">
			  <label class="control-label col-sm-2" for="gender">Gender:</label>
			  <div class="col-sm-10">
			    <label class="radio-inline"><input type="radio" name="person[gender]" value="Male">Male</label>
			    <label class="radio-inline"><input type="radio" name="person[gender]" value="Female">Female</label>
		      </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="birthday">Birth date:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="birthday" name="person[birthdate]" placeholder="Enter Birth Date">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="parent">Parent's Name:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="parent" name="person[parent_name]" placeholder="Enter Parent's Name">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="date_detected">Date of Detection:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="date-detected" name="person[date_detected]" placeholder="Enter Date Detected">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="form_completed_by">Form Completed by:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="form_completed_by" name="person[form_completed_by]" placeholder="Enter Form COmpleted by">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="info_transmitted_by">Information Transmitted By:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="info_transmitted_by" name="person[info_transmitted_by]" placeholder="Enter Information transmitted by">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="notified_by">Notified By:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="notified_by" name="person[notified_by]" placeholder="Enter Notified By">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="notification_group">Notification Group:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="notification_group" name="person[notification_group]" placeholder="Enter Notification Group">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="relationship_to_patient">Relationship to patient:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="relationship_to_patient" name="person[relationship_to_patient]" placeholder="Enter Relationship to patient">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="head_of_household">Head of Houselhold:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="head_of_household" name="person[head_of_household]" placeholder="Enter Head of Household">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="village_district">Village or District:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="village_district" name="person[village_district]" placeholder="Enter Village/District of Residence">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="country">Country:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="country" name="person[country]" placeholder="Enter Country">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="nationality">Nationality:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="nationality" name="person[nationality]" placeholder="Enter Nationality">
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="control-label col-sm-2" for="ethnic_group">Ethnic Group:</label>
		    <div class="col-sm-10"> 
		      <input type="text" class="form-control" id="ethnic_group" name="ethnic_group]" placeholder="Enter Ethnic Group">
		    </div>
		  </div>
		  <div class="form-group"> 
		    <div class="col-sm-offset-2 col-sm-10">
		      <button type="submit" class="btn btn-default">Submit</button>
		    </div>
		  </div>
		</form>
	</div>
</div>
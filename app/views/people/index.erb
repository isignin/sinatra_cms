<h1>List of People</h1>
<table>
<th>ID</th><th>Last Name</th><th>First Name</th><th>Parent's Name</th><th>Birthdate</th>
<% @people.each do |person| %>
    <tr>
		<td><%= person.id %></td><td><%= person.last_name %></td><td><%= person.first_name %></td>
		<td><%= person.parent_name %></td><td><%= person.birthdate %></td><td><a href='/people/contacts/<%= person.id %>'>Contacts</a>
	</tr>
<% end %>  